{
	"name": "weather-api",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"generate:keys": "mkcert create-ca",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"lint": "eslint ./src --cache",
		"lint:fix": "eslint ./src --fix --cache",
		"lint:knip": "knip",
		"lint:prettier": "prettier . --write --cache --log-level silent",
		"lint:spelling": "cspell . --quiet --cache",
		"prepare": "husky install"
	},
	"dependencies": {
		"@nestjs/axios": "^3.0.1",
		"@nestjs/cache-manager": "^2.2.0",
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.1.1",
		"@nestjs/core": "^10.0.0",
		"@nestjs/platform-express": "^10.0.0",
		"axios": "^1.6.5",
		"cache-manager": "^5.3.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"i": "^0.3.7",
		"install": "^0.13.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.4",
		"@commitlint/config-conventional": "^18.4.4",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.2",
		"@types/node": "^20.3.1",
		"@types/supertest": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"cspell": "^8.3.2",
		"cspell-dict-pl_pl": "^1.1.1",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-deprecation": "^2.0.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-jest": "^27.6.1",
		"eslint-plugin-jsdoc": "^48.0.2",
		"eslint-plugin-jsonc": "^2.11.2",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-perfectionist": "^2.5.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-regexp": "^2.2.0",
		"eslint-plugin-sort-destructure-keys": "^1.5.0",
		"eslint-plugin-sort-keys-fix": "latest",
		"eslint-plugin-typescript-sort-keys": "latest",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jsonc-eslint-parser": "^2.4.0",
		"mkcert": "^3.2.0",
		"prettier": "^3.0.0",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
